@model IEnumerable<Afrodite_Sistema.Models.TipoProcedimento>

@{
    ViewData["Title"] = "Index";
}

<h1>Tipo do Procedimento</h1>

<p>
    <a asp-action="Create">Cadastrar Tipo do Procedimento</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NomeTipoProcedimento)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NomeTipoProcedimento)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.TipoProcedimentoId">Editar</a> 
                <a asp-action="Details" asp-route-id="@item.TipoProcedimentoId">Detalhes</a> 
                <a asp-action="Delete" asp-route-id="@item.TipoProcedimentoId">Deletar</a>
            </td>
        </tr>
}
    </tbody>
</table>
